{
  "name": "koa-start-pack",
  "version": "0.0.0",
  "description": "Starting pack for Koa (v.2) including ES6 transpilers",
  "directories": {
    "test": "test"
  },
  "author": "Peter Javorka <peter.javorka@artin.cz>",
